window.oncontextmenu = function(ev) {
  ev.preventDefault();
  ev.stopPropogation();
  return false;
}

document.documentElement.addEventListener("keydown", function(e) {
    if((e.keyCode == 27) || (e.keyCode == 42) || (e.keyCode == 145) || (e.keyCode == 19) || (e.keyCode == 112) || (e.keyCode == 113) || (e.keyCode == 114) || (e.keyCode == 115) || (e.keyCode == 117) || (e.keyCode == 118) || (e.keyCode == 119) || (e.keyCode == 120) || (e.keyCode == 121) || (e.keyCode == 122) || (e.keyCode == 123)) {
        // if F1-F4 and f6-f12 is pressed, don't start the function
        e.preventDefault();
    }
});
